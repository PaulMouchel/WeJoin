<p id="notice"><%= notice %></p>

<h1>Place Tags</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @place_tags.each do |place_tag| %>
      <tr>
        <td><%= link_to 'Show', place_tag %></td>
        <td><%= link_to 'Edit', edit_place_tag_path(place_tag) %></td>
        <td><%= link_to 'Destroy', place_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Place Tag', new_place_tag_path %>
