<h1>New Place</h1>


<% if current_user != nil %>
  <% if @is_admin %>
<%= render 'places/form', model: [:admin, @city, @place], is_admin: @is_admin %>
  <% else %>
<%= render 'places/form', model: [@city, @place], is_admin: @is_admin %>
  <% end %>
<% end %>

<%= link_to 'Back', city_places_path(@city) %>
