<%= form_with(model: city, local: true) do |form| %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :zip_code %>
    <%= form.text_field :zip_code %>
  </div>

  <div class="form-group">
    <%= form.label :city_pic %>
    <%= form.file_field :city_pic, autocomplete: :city_pic %><br />
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
