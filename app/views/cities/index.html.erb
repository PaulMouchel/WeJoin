<p id="notice"><%= notice %></p>

<h1>Cities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cities.each do |city| %>
      <tr>
        <td>
        <%if city.city_pic.attached?%>
          <%= image_tag city.city_pic, alt: 'city' %>
        <%end%></td>
        <td><%= link_to "#{city.name}", city_places_path(city.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
